{
  "name": "@ranavibe/core",
  "version": "2.0.0",
  "description": "RANA Core SDK - Unified LLM client with cost optimization",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "build:no-dts": "tsup src/index.ts --format cjs,esm",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build:no-dts"
  },
  "keywords": [
    "rana",
    "llm",
    "ai",
    "openai",
    "anthropic",
    "gemini",
    "cost-optimization",
    "unified-api"
  ],
  "author": "Waymaker <support@waymaker.cx>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/waymaker-ai/ranavibe.git",
    "directory": "packages/core"
  },
  "homepage": "https://rana.cx",
  "bugs": {
    "url": "https://github.com/waymaker-ai/ranavibe/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "@google/generative-ai": "^0.2.1",
    "glob": "^10.3.10",
    "ioredis": "^5.3.2",
    "js-yaml": "^4.1.0",
    "openai": "^4.28.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.19.25",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^1.3.1"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  }
}
