{
  "name": "@rana/core",
  "version": "2.0.0",
  "description": "RANA Core SDK - Unified LLM client with cost optimization",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "rana",
    "llm",
    "ai",
    "openai",
    "anthropic",
    "gemini",
    "cost-optimization",
    "unified-api"
  ],
  "author": "Waymaker",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "@google/generative-ai": "^0.2.1",
    "ioredis": "^5.3.2",
    "openai": "^4.28.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.19.25",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^1.3.1"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  }
}
