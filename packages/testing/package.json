{
  "name": "@rana/testing",
  "version": "1.0.0",
  "description": "AI-native testing framework - Test AI code like regular code",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "lint": "eslint src/"
  },
  "keywords": [
    "ai",
    "testing",
    "llm",
    "semantic",
    "regression",
    "rana",
    "openai",
    "anthropic"
  ],
  "author": "Waymaker AI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/waymaker-ai/ranavibe.git",
    "directory": "packages/testing"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "cosine-similarity": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "@rana/core": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "@rana/core": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
