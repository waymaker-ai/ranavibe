{
  "name": "@ranavibe/generate",
  "version": "1.0.0",
  "description": "Natural language code generation for RANA Framework",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "rana-generate": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "import": "./dist/cli/index.mjs",
      "require": "./dist/cli/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/cli/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/cli/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "rana",
    "ai",
    "code-generation",
    "llm",
    "natural-language",
    "metaGPT",
    "cursor",
    "copilot"
  ],
  "author": "Waymaker",
  "license": "MIT",
  "dependencies": {
    "@ranavibe/core": "^2.0.0",
    "@ranavibe/helpers": "^1.0.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "glob": "^10.3.10",
    "handlebars": "^4.7.8",
    "inquirer": "^9.2.12",
    "ora": "^8.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/waymaker-ai/ranavibe.git",
    "directory": "packages/generate"
  },
  "homepage": "https://rana.cx",
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}
