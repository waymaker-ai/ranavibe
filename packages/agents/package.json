{
  "name": "@ranavibe/agents",
  "version": "1.0.0",
  "description": "Agent Development Kit (ADK) for RANA Framework",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./presets": {
      "types": "./dist/presets/index.d.ts",
      "import": "./dist/presets/index.mjs",
      "require": "./dist/presets/index.js"
    },
    "./security": {
      "types": "./dist/security/index.d.ts",
      "import": "./dist/security/index.mjs",
      "require": "./dist/security/index.js"
    },
    "./tools": {
      "types": "./dist/tools/index.d.ts",
      "import": "./dist/tools/index.mjs",
      "require": "./dist/tools/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ranavibe/core": "workspace:*",
    "@ranavibe/rag": "workspace:*",
    "zod": "^3.22.4",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "rana",
    "agents",
    "adk",
    "llm",
    "ai",
    "tools",
    "orchestration"
  ],
  "author": "Waymaker <support@waymaker.cx>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/waymaker-ai/ranavibe.git",
    "directory": "packages/agents"
  },
  "homepage": "https://rana.cx",
  "publishConfig": {
    "access": "public"
  }
}
