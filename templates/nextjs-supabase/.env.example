# Supabase Configuration (REQUIRED)
# Get these from: https://supabase.com/dashboard/project/_/settings/api
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Site Configuration (REQUIRED)
NEXT_PUBLIC_SITE_URL=http://localhost:3000
NEXT_PUBLIC_SITE_NAME="My AADS App"
NEXT_PUBLIC_SITE_DESCRIPTION="Built with AADS Framework"

# OpenAI (Optional - for LLM features)
OPENAI_API_KEY=sk-...

# Anthropic Claude (Optional - for LLM features)
ANTHROPIC_API_KEY=sk-ant-...

# xAI Grok (Optional - for LLM features)
XAI_API_KEY=xai-...

# Google Gemini (Optional - for LLM features)
GOOGLE_API_KEY=...

# Upstash Redis (Optional - for caching & rate limiting)
# Get from: https://console.upstash.com
UPSTASH_REDIS_URL=https://...
UPSTASH_REDIS_TOKEN=...

# Resend (Optional - for transactional emails)
# Get from: https://resend.com/api-keys
RESEND_API_KEY=re_...

# Stripe (Optional - for payments)
# Get from: https://dashboard.stripe.com/apikeys
STRIPE_SECRET_KEY=sk_test_...
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...

# Analytics (Optional)
NEXT_PUBLIC_GA_MEASUREMENT_ID=G-...

# Vercel (Auto-populated in production)
VERCEL_URL=
VERCEL_ENV=
